{
  "name": "tactec-website",
  "private": true,
  "version": "1.0.0",
  "description": "TACTEC - Professional football club management platform website",
  "keywords": ["football", "soccer", "club management", "sports", "tactec", "ventio"],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "verify": "node scripts/verify-deployment.js",
    "clean": "rm -rf .next out node_modules/.cache",
    "preinstall": "npx only-allow npm",
    "postbuild": "next-sitemap",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "next-intl": "3.16.0",
    "clsx": "2.1.0",
    "react-hook-form": "^7.49.2",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2"
  },
  "devDependencies": {
    "autoprefixer": "10.4.19",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.10",
    "typescript": "5.4.5",
    "@types/react": "18.2.66",
    "@types/node": "20.11.30",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "next-sitemap": "^4.2.3",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ventio/tactec-website.git"
  },
  "author": {
    "name": "Ventio",
    "url": "https://ventio.com"
  },
  "license": "UNLICENSED",
  "homepage": "https://tactec.club"
}
